{
  "name": "python-line-profiler",
  "displayName": "python-line-profiler",
  "description": "A vscode extension to configure, run, and visualize the results of line-profiler in Python",
  "publisher": "PerpetualHelp",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-line-profiler.registerFunction",
    "onCommand:vscode-line-profiler.unregisterFunction",
    "onCommand:vscode-line-profiler.registerScript",
    "onCommand:vscode-line-profiler.unregisterScript",
    "onCommand:vscode-line-profiler.runAllScripts"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-line-profiler.registerFunction",
        "title": "Python Line Profiler: Register Function"
      },
      {
        "command": "vscode-line-profiler.unregisterFunction",
        "title": "Python Line Profiler: UnRegister Function"
      },
      {
        "command": "vscode-line-profiler.registerScript",
        "title": "Python Line Profiler: Register Script"
      },
      {
        "command": "vscode-line-profiler.unregisterScript",
        "title": "Python Line Profiler: UnRegister Script"
      },
      {
        "command": "vscode-line-profiler.runAllScripts",
        "title": "Python Line Profiler: Run All Scripts"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceLangId == python",
          "command": "vscode-line-profiler.registerScript",
          "group": "1_profile"
        },
        {
          "when": "resourceLangId == python",
          "command": "vscode-line-profiler.unregisterScript",
          "group": "1_profile"
        },
        {
          "when": "resourceLangId == python",
          "command": "vscode-line-profiler.runAllScripts",
          "group": "1_profile"
        }
      ],
      "editor/context": [
        {
          "when": "resourceLangId == python",
          "command": "vscode-line-profiler.registerFunction",
          "group": "1_profile"
        },
        {
          "when": "resourceLangId == python",
          "command": "vscode-line-profiler.unregisterFunction",
          "group": "1_profile"
        },
        {
          "when": "resourceLangId == python",
          "command": "vscode-line-profiler.runAllScripts",
          "group": "1_profile"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "npm run build && tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "python": "cp -r ./src/extension.py ./out",
    "build": "npm run compile && npm run python"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/node-fetch": "2.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "json-schema-to-typescript": "^11.0.2",
    "mocha": "^10.0.0",
    "node-fetch": "^2.6.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "vsce": "^2.11.0"
  }
}
